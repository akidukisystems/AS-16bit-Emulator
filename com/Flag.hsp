    if ( vbin_CPUmeirei_Main == 0x88 ) : { // SAHF

        vbin_CPUtmp = fetchResistorWe(rFL)
        vbin_CPUtmp.1 = fetchResistorWg(rAH)

        if ( vbin_CPUtmp.1 && 1 ) {

            vbin_CPUtmp = vbin_CPUtmp || fCF

        } else {

            vbin_CPUtmp = vbin_CPUtmp && notx(fCF)

