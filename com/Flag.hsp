    if ( vbin_CPUmeirei_Main == 0x87 ) : { // LAHF

        vbin_CPUtmp = %00000010
        vbin_CPUtmp.1 = fetchResistorWe(rFL)

        if ( vbin_CPUtmp.1 && fCF ) {

